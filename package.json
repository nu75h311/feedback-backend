{
  "name": "feedback-backend",
  "version": "0.1.0",
  "description": "Feedback app backend.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nu75h311/feedback-backend.git"
  },
  "author": "Adriano Tanoue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nu75h311/feedback-backend/issues"
  },
  "homepage": "https://github.com/nu75h311/feedback-backend#readme",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --fix --project \"tsconfig.json\"",
    "serve": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "start": "nodemon --config \"./util/nodemon.json\"",
    "test": "cross-env ENV_FILE=test jest --setupFiles ./env",
    "test-watch": "jest --watch"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "module-alias": "^2.2.2",
    "mongoose": "^5.7.6",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.4.4",
    "@types/jest": "^24.0.19",
    "@types/jsonfile": "^5.0.0",
    "@types/mongoose": "^5.5.23",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.11.6",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.5.0",
    "jest": "^24.9.0",
    "jest-html-reporter": "^2.6.2",
    "jsonfile": "^5.0.0",
    "mongodb-memory-server": "^5.2.11",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
